<!DOCTYPE html>
<html lang="en">
<head>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="stylesheet" href="/stylesheets/style2.css" />
        <meta name="referrer" content="no-referrer"> <!-- this is placed here to override the 403 error with the imigur-->
    <title>Community Gallery</title>
</head>
<body>
    <img src="/assets/banner-logo.png"/>
    <%- include('../partials/nav.ejs') %> 
    <h1> Community Art Gallery</h1>   
    <div class="gallery-container">
        <% users.forEach((user) => { %>
            <% user.userGallery.forEach((artwork) => {%>
            <div>  
          <img src="<%= artwork.artworkImage %>"></img>  
          <p>
            <a script="/artwork/<%= artwork._id %>"><%= artwork.artworkTitle %></a>
          </p>
          </div>
            <% }) %>
            <%}) %>
    </div>
</body>
</html>